{
  "name": "trilium-cli-ts",
  "version": "0.1.0",
  "workspaces": [
    "packages/*"
  ],
  "description": "A TypeScript CLI and TUI client for Trilium Notes",
  "keywords": [
    "trilium",
    "notes",
    "cli",
    "tui",
    "typescript"
  ],
  "author": "Your Name <you@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/trilium-cli-ts"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "type": "module",
  "main": "./dist/lib/index.js",
  "types": "./dist/lib/index.d.ts",
  "bin": {
    "trilium": "./dist/bin/trilium.js",
    "trilium-core": "./dist-core/core-cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/lib/index.js",
      "types": "./dist/lib/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build:packages": "npm run build --workspaces",
    "build": "npm run build:packages && tsc && chmod +x dist/bin/trilium.js",
    "build:watch": "tsc --watch",
    "build:minimal": "tsc -p tsconfig.minimal.json && chmod +x dist/minimal-cli.js",
    "build:core": "tsc -p tsconfig.core.json && chmod +x dist-core/core-cli.js",
    "build:core:watch": "tsc -p tsconfig.core.json --watch",
    "dev": "tsx src/bin/trilium.ts",
    "dev:watch": "tsx watch src/bin/trilium.ts",
    "dev:minimal": "tsx src/minimal-cli.ts",
    "dev:core": "tsx src/core-cli.ts",
    "start": "node dist/bin/trilium.js",
    "start:minimal": "node dist/minimal-cli.js",
    "start:core": "node dist-core/core-cli.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:unit": "vitest run test/api test/utils test/cli --exclude test/integration",
    "test:integration": "vitest run test/integration --reporter=verbose",
    "test:integration:watch": "vitest test/integration --reporter=verbose",
    "test:ci": "./test/ci-test.sh",
    "test:coverage": "vitest run --coverage",
    "test:minimal": "vitest src/minimal-cli.test.ts --run",
    "test:performance": "vitest run test/integration/performance-benchmark.test.ts --reporter=verbose",
    "test:errors": "vitest run test/integration/error-handling.test.ts --reporter=verbose",
    "test:live": "tsx test/integration/run-live-tests.ts",
    "test:live:debug": "TRILIUM_TEST_DEBUG=true tsx test/integration/run-live-tests.ts",
    "test:live:no-cleanup": "TRILIUM_TEST_CLEANUP=false tsx test/integration/run-live-tests.ts",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json}\"",
    "typecheck": "tsc --noEmit",
    "typecheck:minimal": "tsc -p tsconfig.minimal.json --noEmit",
    "clean": "rm -rf dist",
    "validate": "npm run typecheck && npm run build",
    "validate:with-lint": "npm run typecheck && npm run lint && npm run build",
    "validate:full": "npm run clean && npm run validate && npm test -- --run",
    "validate:minimal": "npm run typecheck:minimal && npm run build:minimal && npm run test:minimal",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@trilium-cli/zod": "file:./packages/trilium-zod",
    "@trilium-cli/tui": "file:./packages/trilium-tui",
    "@clack/prompts": "^0.8.2",
    "@types/marked": "^5.0.2",
    "@types/node": "^22.0.0",
    "@types/react": "^18.3.23",
    "adm-zip": "^0.5.10",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "conf": "^12.0.0",
    "console-table-printer": "^2.12.1",
    "date-fns": "^3.3.1",
    "dotenv": "^16.4.1",
    "enquirer": "^2.4.1",
    "file-type": "^19.0.0",
    "fuzzy": "^0.1.3",
    "glob": "^10.3.10",
    "globby": "^14.0.1",
    "got": "^14.2.1",
    "gray-matter": "^4.0.3",
    "ink": "^4.4.1",
    "ink-select-input": "^5.0.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^5.0.1",
    "inquirer": "^9.2.15",
    "js-yaml": "^4.1.0",
    "jsdom": "^24.0.0",
    "listr2": "^8.0.1",
    "marked": "^16.2.0",
    "mime-types": "^2.1.35",
    "nanoid": "^5.0.5",
    "ora": "^8.0.1",
    "p-limit": "^5.0.0",
    "p-queue": "^8.0.1",
    "react": "^18.3.1",
    "rimraf": "^5.0.5",
    "turndown": "^7.1.3",
    "update-notifier": "^7.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.5",
    "@types/glob": "^8.1.0",
    "@types/inquirer": "^9.0.7",
    "@types/js-yaml": "^4.0.9",
    "@types/jsdom": "^21.1.6",
    "@types/mime-types": "^2.1.4",
    "@types/turndown": "^5.0.4",
    "@types/update-notifier": "^6.0.8",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitest/coverage-v8": "^1.3.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "prettier": "^3.2.5",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "vitest": "^1.3.0"
  }
}
